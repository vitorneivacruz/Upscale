{
  "name": "caas2-webapp",
  "version": "0.54.0-20210706193449-stg",
  "license": "MIT",
  "scripts": {
    "upscale-style-ext": "node scripts/upscale-style-ext.js",
    "postinstall": "npm run upscale-style-ext",
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=3084 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve",
    "lint": "eslint -c .eslintrc.js --ext .ts src/app",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "node scripts/version-author.js",
    "test": "ng test",
    "test:once": "npm run test -- --watch=false",
    "test:headed": "npm run test -- --browsers Chrome",
    "test:coverage": "npm run test -- --code-coverage --watch=true",
    "test:coverage-once": "npm run test -- --code-coverage --watch=false",
    "e2e": "ng e2e",
    "doc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "dev:ssr": "npm run ng-high-memory -- run upscale-webapp:serve-ssr",
    "serve:ssr": "node dist-server/main.js",
    "build:ssr": "npm run package:serverless && ng run upscale-webapp:server:production",
    "package": "ng build",
    "package:prod": "ng build --prod --configuration=production",
    "package:serverless": "ng build --prod --configuration=serverless"
  },
  "private": false,
  "dependencies": {
